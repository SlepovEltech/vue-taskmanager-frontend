AQA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,MAAA,eAAA,CAAA,MAAA,CAAA,IAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,IAAA,CAAA,EAAA,GAAA,QAAA,CAAA,IAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,IAAA;AACA,OALA,EAMA,KANA,CAMA,UAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,OARA;AASA,WAAA,OAAA,CAAA,IAAA,CAAA,OAAA;AACA;AArBA,GAlBA;AA0CA,EAAA,SA1CA,uBA0CA;AACA,QAAA,KAAA,IAAA,IAAA,KAAA,IAAA,CAAA,OAAA,EAAA;AACA,WAAA,IAAA,CAAA,OAAA;AACA;;AACA,QAAA,KAAA,KAAA,IAAA,KAAA,KAAA,CAAA,OAAA,EAAA;AACA,WAAA,KAAA,CAAA,OAAA;AACA;AACA;AAjDA,CAAA","sourcesContent":["<template>\n  <div class=\"row\">\n    <div class=\"col s6 offset-s3\">\n      <h1>Create task</h1>\n      <form @submit.prevent=\"submitHandler\">\n        <div class=\"input-field\">\n          <input  v-model=\"title\" id=\"title\" type=\"text\" class=\"validate\" required>\n          <label for=\"title\" class=\"active\">Title</label>\n          <span class=\"helper-text\" data-error=\"Title is required\" ></span>\n          <span class=\"character-counter\" style=\"float: right; font-size: 12px;\">{{title.length}}/128</span>\n\n        </div>\n\n        <div class=\"chips\" ref=\"chips\"></div>\n\n        <div class=\"input-field\">\n          <textarea v-model=\"description\" id=\"description\" class=\"materialize-textarea\"></textarea>\n          <label for=\"description\">Task description</label>\n          <span class=\"character-counter\" style=\"float: right; font-size: 12px;\">{{description.length}}/2048</span>\n        </div>\n\n        <input type=\"text\" ref=\"datepicker\">\n        <button class=\"btn\" type=\"submit\">Submit task</button>\n      </form>\n    </div>\n  </div>\n</template>\n\n\n<script>\nimport TaskDataService from \"@/services/TaskDataService\";\n\nexport default {\n  name: \"Create\",\n  data: () => ({\n    description: '',\n    title: '',\n    chips: null,\n    date: null,\n  }),\n  mounted(){\n    this.chips = M.Chips.init(this.$refs.chips , {\n      placeholder: 'Task tags'\n    })\n    this.date = M.Datepicker.init(this.$refs.datepicker, {\n      format: 'dd.mm.yyyy',\n      defaultDate: new Date(),\n      setDefaultDate: true\n    })\n  },\n  methods:{\n    submitHandler(){\n      const task ={\n        title : this.title,\n        description : this.description,\n        id : Date.now(),\n        completed : false,\n        tags : this.chips.chipsData,\n        deadline : this.date.date\n      }\n      console.log(task);\n      TaskDataService.create(task)\n          .then(response => {\n            task.id = response.data.id;\n            console.log(response.data);\n            this.submitted = true;\n          })\n          .catch(e => {\n            console.log(e);\n          });\n      this.$router.push('/list');\n    },\n\n  },\n  destroyed(){\n    if(this.date && this.date.destroy){\n      this.date.destroy()\n    }\n    if(this.chips && this.chips.destroy){\n      this.chips.destroy()\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/views"}]}