    placeholder: 'Task tags'\n    })\n    this.date = M.Datepicker.init(this.$refs.datepicker, {\n      format: 'dd.mm.yyyy',\n      defaultDate: new Date(),\n      setDefaultDate: true\n    })\n    this.getTask();\n  },\n  methods:{\n    getTask() {\n      return TaskDataService.get(+this.$route.params.id)\n          .then(response => {\n            this.task = response.data;\n            console.log(response.data);\n          })\n          .catch(e => {\n            console.log(e);\n          });\n    },\n    submitHandler(){\n      const task ={\n        title : this.title,\n        description : this.description,\n        id : Date.now(),\n        completed : false,\n        tags : this.chips.chipsData,\n        deadline : this.date.date\n      }\n      console.log(task);\n      TaskDataService.create(task)\n          .then(response => {\n            task.id = response.data.id;\n            console.log(response.data);\n            this.submitted = true;\n          })\n          .catch(e => {\n            console.log(e);\n          });\n      this.$router.push('/list');\n    },\n\n  },\n  destroyed(){\n    if(this.date && this.date.destroy){\n      this.date.destroy()\n    }\n    if(this.chips && this.chips.destroy){\n      this.chips.destroy()\n    }\n  }\n\n}\n</script>\n\n<style scoped>\n\n</style>"],"sourceRoot":"src/views"}]}